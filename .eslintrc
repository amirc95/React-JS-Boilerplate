{
  "parser": "babel-eslint",
  "plugins": ["jest"],
  "extends": ["airbnb", "prettier", "plugin:jest/recommended"],
  "rules": {
    "linebreak-style": 0,
    "import/no-unresolved": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "env": {
    "jest/globals": true,
    "browser": true,
    "es6": true,
    "node": true
  }
}
